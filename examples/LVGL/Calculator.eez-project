          "customInputs": [],
          "customOutputs": [],
          "description": "Do the operation",
          "tests": [
            {
              "objID": "equals-add-0001-0000-00000000",
              "condition": "operator == \"+\"",
              "outputName": "ADD",
              "outputValue": "operand + Flow.parseFloat(display)"
            },
            {
              "objID": "equals-sub-0001-0000-00000000",
              "condition": "operator == \"-\"",
              "outputName": "SUB",
              "outputValue": "operand - Flow.parseFloat(display)"
